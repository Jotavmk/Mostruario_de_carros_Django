{
    "builds": [
        {
            "src": "loja_carros/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "loja_carros/wsgi.py"
        }
    ],
    "functions": {
        "loja_carros/wsgi.py": {
            "maxDuration": 30
        }
    },
    "buildCommand": "bash vercel_build.sh"
} 